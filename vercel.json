{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/history.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/latest", "dest": "api/index.js" },
    { "src": "/", "dest": "api/index.js" },
    { "src": "/history", "dest": "api/history.js" }
  ]
}